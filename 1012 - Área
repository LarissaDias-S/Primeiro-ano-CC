#include <stdio.h>
#include <stdlib.h>

int main()
{
  double a, b, c, tria, cir, tra, qua, ret;

  scanf("%lf %lf %lf", &a, &b, &c);

  tria = (a*c)/2;
  cir = 3.14159 * c * c;
  tra = ((a + b) * c)/2;
  qua = b*b;
  ret = a*b;


  printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", tria, cir, tra, qua, ret);

    return 0;
}
