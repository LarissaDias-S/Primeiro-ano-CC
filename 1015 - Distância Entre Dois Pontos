#include <stdio.h>
#include <stdlib.h>
#include <math.h>
 
double dist(double x1, double y1, double x2, double y2){
    
    double d = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));
    
    return d;
}
 
int main() {
 double a1, b1, a2, b2;

scanf("%lf %lf %lf %lf", &a1, &b1, &a2, &b2);
 
 printf("%.4lf\n", dist(a1, b1, a2, b2));
 
    return 0;
}
