#include <stdio.h>
#include <stdlib.h>
#include <math.h>
 
 
int main() {
 double A, B, C, total1, total2;
 
 scanf("%lf %lf %lf", &A, &B, &C);
 
    total1 = ((-B) + sqrt((B*B) - (4 * A * C)))/(2*A);
    total2 = ((-B) - sqrt((B*B) - (4 * A * C)))/(2*A);

 if((2*A == 0) || (( (B*B) - (4 * A * C)) < 0.0)){
     
     printf("Impossivel calcular\n");
     
 }
 else{
 printf("R1 = %.5lf\nR2 = %.5lf\n", total1, total2);
 }
 
    return 0;
}
