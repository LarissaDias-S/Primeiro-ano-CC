int main()
{
    int n=1, alcool=0, gas=0, diesel=0;

   while(n != 4) {
    scanf("%d", &n);

    if(n == 1) {
    alcool++;

    }
    else if(n == 2) {
    gas++;

    }
    else if(n == 3) {
    diesel++;

    }

  }

    printf("MUITO OBRIGADO\nAlcool: %d\nGasolina: %d\nDiesel: %d\n", alcool, gas, diesel);

    return 0;
}
