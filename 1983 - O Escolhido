#include <stdio.h>
 
int main()
{
    int al, nMat, maiorMat=0;
    float nota, maiorN=0.00;

    scanf("%d", &al);

    for(int i = 0; i < al; i++)
    {
        scanf("%d %f", &nMat, &nota);

        if(nota > maiorN)
        {
            maiorN = nota;
            maiorMat = nMat;
        }


    }

    if(maiorN >= 8.0)
    {
        printf("%d\n", maiorMat);
    }
    else
    {
        printf("Minimum note not reached\n");
    }

    return 0;

}
